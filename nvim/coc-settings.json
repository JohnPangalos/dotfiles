{
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "svelte"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "svelte"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescriptreact",
    "svelte",
    "go",
    "javascript",
    "json",
    "html",
    "svg",
    "markdown"
  ],
  "typescript.suggest.autoImports": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    },

    "terraform": {
      "command": "terraform-ls",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    }
  }
}
