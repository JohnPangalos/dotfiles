{
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "svelte"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "svelte"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescriptreact",
    "svelte",
    "go",
    "javascript",
    "json",
    "html",
    "svg",
    "markdown"
  ],
  "go.goplsOptions": {
    "gofumpt": true
  },
  "tailwindCSS.headwind.runOnSave": false,
  "tailwindCSS.jsLanguages": [
    "javascript",
    "javascriptreact",
    "reason",
    "typescriptreact",
    "svelte"
  ],
  "tailwindCSS.htmlLanguages": [
    "blade",
    "edge",
    "eelixir",
    "ejs",
    "elixir",
    "elm",
    "erb",
    "eruby",
    "haml",
    "handlebars",
    "htmldjango",
    "html",
    "HTML (EEx)",
    "HTML (Eex)",
    "html.twig",
    "jade",
    "leaf",
    "markdown",
    "njk",
    "nunjucks",
    "php",
    "razor",
    "slim",
    "svelte",
    "twig",
    "vue"
  ],
  "typescript.suggest.autoImports": true,
  "prettier.proseWrap": "always"
}
